# platform :ios, '9.0'

def iOSWork_pod
pod 'LookinServer', :configurations => ['Debug']
pod "PromiseKit"
pod "Alamofire"
pod "Gifu"
pod "Kanna"
pod "IQKeyboardManagerSwift"
pod "Kingfisher"
pod "KRProgressHUD"
pod "SnapKit"
pod "SkeletonView"
pod "SwiftyJSON"
pod "SwiftSoup"
end

target 'CLangWork' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for CLangWork

end

target 'ConsoleWork' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for ConsoleWork

end

target 'iOSWork' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
  iOSWork_pod
  # Pods for iOSWork

end

